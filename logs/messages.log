    } catch (err) {
        console.error('[JADIBOT] Start error:', err);
        qrTracker.delete(userId); // âœ… Clear tracker on error
        await mainSock.sendMessage(from, { 
            text: `âŒ Error memulai jadibot:\n\n${err.message}` 
        });
    }
}

