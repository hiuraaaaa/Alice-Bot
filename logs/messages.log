const jadibotSessions = new Map();
const msgRetryCounterCache = new NodeCache();
const qrTracker = new Map(); // âœ… Track QR per user

async function startJadibot(userId, mainSock, from) {
    try {
        if (isConnected(userId)) {
            await mainSock.sendMessage(from, { 
                text: 'âŒ Kamu sudah terhubung sebagai bot!\n\nGunakan `stopjadibot` untuk disconnect.' 
