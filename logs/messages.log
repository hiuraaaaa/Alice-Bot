                keys: makeCacheableSignalKeyStore(state.keys, pino({ level: "silent" }))
            },
            msgRetryCounterCache,
            generateHighQualityLinkPreview: true,
            getMessage: async (key) => {
                return { conversation: 'Jadibot Session' };
            }
        });

        jadibotSessions.set(userId, sock);

        // âœ… Initialize QR tracker
        if (!qrTracker.has(userId)) {
            qrTracker.set(userId, {
                count: 0,
                lastSent: 0,
                maxQR: 3 // Max 3 QR codes
            });
        }

        sock.ev.on('connection.update', async (update) => {
            const { connection, lastDisconnect, qr } = update;

            // âœ… HANDLE QR WITH RATE LIMITING
            if (qr) {
                const tracker = qrTracker.get(userId);
                const now = Date.now();
                
                // Check if max QR reached
                if (tracker.count >= tracker.maxQR) {
                    console.log(`[JADIBOT] Max QR reached for ${userId}`);
                    
                    jadibotSessions.delete(userId);
                    qrTracker.delete(userId);
                    
                    await mainSock.sendMessage(from, {
                        text: 
                            `âš ï¸ *JADIBOT TIMEOUT*\n\n` +
                            `QR code sudah expired ${tracker.maxQR} kali.\n\n` +
                            `Kemungkinan:\n` +
                            `â€¢ Scan terlalu lama\n` +
                            `â€¢ Koneksi tidak stabil\n\n` +
                            `Gunakan \`jadibot\` untuk coba lagi.`
                    });
                    
                    // Force close socket
                    try {
                        await sock.logout();
                    } catch (e) {}
                    
                    return;
                }

                // Rate limit: 15 detik antar QR
                if (now - tracker.lastSent < 15000) {
                    console.log(`[JADIBOT] QR rate limited for ${userId}`);
                    return;
                }

                try {
                    const qrImage = await qrcode.toDataURL(qr);
                    const qrBuffer = Buffer.from(qrImage.split(',')[1], 'base64');

                    await mainSock.sendMessage(from, {
                        image: qrBuffer,
                        caption: 
                            `*ğŸ¤– JADIBOT - SCAN QR* (${tracker.count + 1}/${tracker.maxQR})\n\n` +
                            `Scan QR ini dengan WhatsApp kamu:\n\n` +
                            `1. Buka WhatsApp di HP\n` +
                            `2. Tap Menu (â‹®) > Linked Devices\n` +
                            `3. Tap "Link a Device"\n` +
                            `4. Scan QR code di atas\n\n` +
                            `â³ QR expired dalam 60 detik\n` +
                            `âš ï¸ Sisa kesempatan: ${tracker.maxQR - tracker.count - 1}\n\n` +
                            `_Scan cepat sebelum expired!_`
                    });

                    // Update tracker
                    tracker.count++;
                    tracker.lastSent = now;
                    qrTracker.set(userId, tracker);

                    console.log(`[JADIBOT] QR sent to ${userId} (${tracker.count}/${tracker.maxQR})`);

                } catch (err) {
                    console.error('[JADIBOT] QR send error:', err);
                }
            }

            // Connected
            if (connection === 'open') {
                const user = sock.user;
                
                // âœ… Clear QR tracker saat berhasil connect
                qrTracker.delete(userId);
                
                addConnection(userId, {
                    jid: user.id,
                    name: user.name || 'Unknown',
                    number: user.id.split(':')[0]
                });

                await mainSock.sendMessage(from, {
                    text: 
                        `âœ… *JADIBOT BERHASIL!*\n\n` +
                        `ğŸ“± Nomor: ${user.id.split(':')[0]}\n` +
                        `ğŸ‘¤ Nama: ${user.name || 'Unknown'}\n` +
                        `â° Waktu: ${new Date().toLocaleString('id-ID')}\n\n` +
                        `Bot kamu sekarang aktif!\n\n` +
                        `*Commands:*\n` +
                        `â€¢ stopjadibot - Disconnect\n` +
                        `â€¢ listjadibot - Lihat jadibot aktif\n\n` +
                        `_Jangan logout dari WhatsApp!_`
                });

                console.log(`[JADIBOT] âœ… Connected: ${user.id}`);
            }

            // Disconnected
            if (connection === 'close') {
                const statusCode = lastDisconnect?.error?.output?.statusCode;
                const shouldReconnect = statusCode !== DisconnectReason.loggedOut;
                
                console.log('[JADIBOT] Connection closed. Status:', statusCode);

                // âœ… Clear tracker saat disconnect
                qrTracker.delete(userId);

                if (shouldReconnect) {
                    console.log('[JADIBOT] Reconnecting...');
                    setTimeout(() => startJadibot(userId, mainSock, from), 3000);
                } else {
                    removeConnection(userId);
                    jadibotSessions.delete(userId);
                    
                    await mainSock.sendMessage(from, {
                        text: 
                            `âš ï¸ *JADIBOT DISCONNECTED*\n\n` +
                            `Bot kamu telah logout.\n\n` +
                            `Gunakan \`jadibot\` untuk connect lagi.`
                    });

                    console.log(`[JADIBOT] âŒ Logged out: ${userId}`);
                }
            }
        });

        sock.ev.on('creds.update', saveCreds);

        sock.ev.on('messages.upsert', async ({ messages }) => {
            const msg = messages[0];
            if (!msg.message || msg.key.fromMe) return;
            console.log(`[JADIBOT ${userId}] Message from ${msg.key.remoteJid}`);
        });

        await mainSock.sendMessage(from, { 
            text: 
                `â³ *MEMULAI JADIBOT...*\n\n` +
                `Tunggu QR code...\n\n` +
                `_Pastikan nomor kamu belum logout_` 
