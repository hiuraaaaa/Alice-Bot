  if (!args[0]) return m.reply(`Kirim link YouTube! Contoh: *${global.prefix + command} https://youtu.be/...*`);

  await sock.sendMessage(m.chat, { react: { text: "ğŸ“¥", key: m.key } });

  try {
    const url = args[0];
    
    // 1. Ambil metadata via oEmbed
    const metaRes = await fetch(`https://www.youtube.com/oembed?url=${encodeURIComponent(url)}&format=json`);
